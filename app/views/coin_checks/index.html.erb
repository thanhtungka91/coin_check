<p id="notice"><%= notice %></p>

<h1>Coin Checks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order["id"] %></td>
        <td><%= order["order_type"]%></td>
        <td><%= order["rate"]%></td>
        <td><%= order["pair"]%></td>
        <td><%= order["pending_amount"]%></td>
        <td><%= order["created_at"]%></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
